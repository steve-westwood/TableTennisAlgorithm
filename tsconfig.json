{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es2019",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "types": ["node", "jest"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
  }
}